{
  "name": "wolkenkit-chat-auth0",
  "version": "1.0.0",
  "description": "wolkenkit-chat-auth0 is a chat using wolkenkit and Auth0.",
  "contributors": [
    {
      "name": "Golo Roden",
      "email": "golo.roden@thenativeweb.io"
    }
  ],
  "wolkenkit": {
    "application": "chat",
    "runtime": {
      "version": "1.2.0"
    },
    "environments": {
      "default": {
        "api": {
          "address": {
            "host": "local.wolkenkit.io",
            "port": 3000
          },
          "allowAccessFrom": "*"
        },
        "identityProvider": {
          "name": "https://thenativeweb.eu.auth0.com/",
          "certificate": "/server/keys/auth0"
        },
        "node": {
          "environment": "development"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thenativeweb/wolkenkit-chat-auth0.git"
  },
  "keywords": [
    "wolkenkit"
  ],
  "license": "AGPL-3.0"
}
